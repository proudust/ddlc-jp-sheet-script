{
  "name": "gas-clasp-starter",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "fix": "run-s fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix .",
    "fix:prettier": "prettier --write --ignore-path .gitignore .",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "test": "jest",
    "build": "rimraf dist && webpack && cpy src/appsscript.json dist",
    "deploy": "ts-node -O '{\"module\":\"commonjs\"}' deploy.ts"
  },
  "author": "proudust",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "^2.3.0",
    "@types/google-apps-script": "^1.0.36",
    "@types/jest": "^26.0.15",
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.13.0",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-jest": "^24.3.6",
    "gas-webpack-plugin": "^1.2.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
