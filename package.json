{
  "name": "gas-clasp-starter",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "fix": "run-s fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix .",
    "fix:prettier": "prettier --write --ignore-path .gitignore .",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "test": "jest",
    "build": "rimraf dist && webpack && cpy src/appsscript.json dist",
    "deploy": "ts-node -O '{\"module\":\"commonjs\"}' deploy.ts"
  },
  "author": "proudust",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "2.3.0",
    "@types/google-apps-script": "1.0.27",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "cpy-cli": "^3.1.1",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-googleappsscript": "1.0.3",
    "eslint-plugin-jest": "24.1.5",
    "gas-webpack-plugin": "2.0.0",
    "jest": "26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.17",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack": "^5.24.3",
    "webpack-cli": "4.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
