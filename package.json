{
  "name": "ddlc-jp-sheet-script",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore src",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "test": "run-p -l -c --aggregate-output lint:* unit",
    "unit": "jest",
    "build": "webpack",
    "deploy": "ts-node -O '{\"module\":\"commonjs\"}' deploy.ts"
  },
  "author": "proudust",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "^2.4.1",
    "@types/google-apps-script": "^1.0.41",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^8.3.0",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-jest": "^25.3.0",
    "filemanager-webpack-plugin": "^6.1.7",
    "gas-webpack-plugin": "^2.1.0",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
